<mvc:View
	controllerName=""
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<Select 
		id="selectRegion"
		selectedKey="{RegionText}"
		items="{
			path: '/zjblessons_base_Regions'
		}"
	>
		<items>
			<c:Item key="{RegionId}" text="{RegionText}" />
		</items>
    </Select>
</mvc:View>