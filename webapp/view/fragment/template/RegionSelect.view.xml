<mvc:View
	controllerName=""
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<Select 
		id="selectRegion"
		selectedKey="{RegionText}"
	>
		<items>
			<c:Item key="{RegionId}" text="{RegionName}" />
		</items>
    </Select>
</mvc:View>