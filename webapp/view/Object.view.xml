<mvc:View
	controllerName="zjblessons.Worklist.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<Page
		showFooter="false"
	>
		<content>
			<IconTabBar
				expandable="false"
				headerMode="Inline"
				select="onIconTabBarSelect"
				selectedKey="{= ${objectView/>sSelectedTab}}"

			>
				<items>
					<IconTabFilter 
						icon="sap-icon://list"
						key="List"
						text="List"
					>
					</IconTabFilter>
					<IconTabFilter 
						icon="sap-icon://form"
						key="Form"
						text="Form"
					>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
	</Page>

</mvc:View>