<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>
    <ComboBox id="comboBoxPlant" 
              selectedKey="{PlantText}" 
              placeholder="Select plant" 
              width="50%"
              items="{
                path: '/zjblessons_base_Plants'
              }">
        <items>
            <c:Item key="{PlantId}" text="{PlantText}" />
        </items>
    </ComboBox>
</c:FragmentDefinition>